@model IEnumerable<CarDealership.Models.Special>
@{
    ViewBag.Title = "Specials";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container">
    <h2>Specials</h2>
    <br />
    <div id="contentTiles">
        @foreach (var special in Model)
        {
            {
                Html.RenderPartial("_SpecialBox", special);
            }
        }
    </div>
</div>



